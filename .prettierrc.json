{
  "printWidth": 120,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "never",
  "overrides": [
    {
      "files": ["*.js", "*.ts"],
      "options": {
        "singleQuote": true
      }
    }
  ],
  "plugins": []
}
